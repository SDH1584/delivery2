<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main">
	
	<!-- 지도 디폴트 위치 정보 가져오기 -->
	<select id="selectMarker"  resultType="MainVo">
		<![CDATA[
			select storeame,store_phone,store_m_adr,store_s_adr,store_lat,store_lng
			from business
		]]>
	</select>
	
	<!-- 메인화면 가게정보가져오기 -->
	<select id="storeList" resultType="MainVo">
			<![CDATA[
			select store_name,store_phone,store_m_adr,store_s_adr,store_phone
			from business
		]]>
	</select>
	
</mapper>
